{
  "manifest_version": 3,
  "name": "React WhatsApp Sender",
  "version": "1.0",
  "description": "Send WhatsApp messages via Chrome extension",
  "permissions": ["scripting", "tabs", "activeTab"],
  "host_permissions": ["https://web.whatsapp.com/*"],
  "action": {
    "default_popup": "index.html"
  }
}
